<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/splash"
        android:label="SmartBin"
        android:roundIcon="@drawable/splash"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartBin"
        tools:targetApi="31" >

        <activity
            android:name=".choose_login.ChooseLoginActivity" android:exported="false"
            android:theme="@style/Theme.SmartBin"/>

        <activity
            android:name=".dasboard.DasboardActivity"
            android:exported="false"
            android:theme="@style/Theme.SmartBin" />

        <activity
            android:name=".register.RegisterActivity"
            android:exported="false"
            android:theme="@style/Theme.SmartBin" />

        <activity
            android:name=".login.LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.SmartBin" />

        <activity
            android:name=".spalshscreen.SplashScreenActivity" android:exported="true"
            android:theme="@style/Theme.SmartBin" > <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity
            android:name=".main.MainActivity"
            android:exported="false"
            android:theme="@style/Theme.SmartBin" />

    </application>

</manifest>